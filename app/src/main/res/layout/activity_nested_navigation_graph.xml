<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".intent_practice.navigationGraph.activity.NestedNavigationGraph">

    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:id="@+id/btn_accept_privacy"
        style="@style/Widget.Material3.Button"
        tools:text="Enter Name Here"
        android:text="@string/btn_accept_terms"
        android:textSize="@dimen/_15sp"
        android:layout_marginHorizontal="@dimen/dp_10"
        android:backgroundTint="@color/bank_navy_blue"
        app:cornerRadius="@dimen/dp_10"
        android:layout_marginVertical="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:id="@+id/btn_denied_privacy"
        android:text="@string/btn_denied_terms"
        style="@style/Widget.Material3.Button"
        tools:text="Enter Name Here"
        android:textSize="@dimen/_15sp"
        android:layout_marginHorizontal="@dimen/dp_10"
        android:backgroundTint="@color/bank_navy_blue"
        app:cornerRadius="@dimen/dp_10"
        android:layout_marginVertical="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/btn_accept_privacy"/>

    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:id="@+id/btn_nested_graph"
        android:text="@string/btn_nested_graph"
        style="@style/Widget.Material3.Button"
        tools:text="Enter Name Here"
        android:textSize="@dimen/_15sp"
        android:layout_marginHorizontal="@dimen/dp_10"
        android:backgroundTint="@color/bank_navy_blue"
        app:cornerRadius="@dimen/dp_10"
        android:layout_marginVertical="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/btn_denied_privacy"/>


</androidx.constraintlayout.widget.ConstraintLayout>